<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">

	<class name="com.bplow.report.vo.ReportParameterMap" table="REPORT_PARAMETER_MAP">
		
		<id name="id" column="ID" type="java.lang.String">            
			<generator class="uuid.hex"/>            
        </id>
		
        <property name="reportId" column="REPORT_ID" not-null="true" unique="false"/>
        
        <property name="parameterId" column="PARAMETER_ID" not-null="true" unique="false"/>
        
        <property name="parameterName" column="PARAMETER_NAME" not-null="true" unique="false"/>
        
        <property name="required" column="REQUIRED" not-null="false" unique="false"/>
        
        <property name="sortOrder" column="SORT_ORDER" not-null="false" unique="false"/>
        
    </class>
    
</hibernate-mapping>
