<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true">

	<class name="com.bplow.report.vo.Report" table="REPORT">
		
		<id name="id" column="REPORT_ID" type="java.lang.String">            
			<generator class="uuid.hex"/>          
        </id>
        
        <property name="name" column="NAME" not-null="true" unique="true"/>
        
        <property name="description" column="DESCRIPTION" not-null="true" unique="false"/>
        
        <property name="reportGroupId" column="REPORT_GROUP_ID" type="text" />
        
        <property name="reportGroupName" column="REPORT_GROUP_NAME" type="text" />
        
        <property name="file" column="REPORT_FILE" not-null="true" unique="false"/>
        
        <property name="query" column="REPORT_QUERY" type="text" /> 
                
        <property name="dataSourceId" column="DATASOURCE_ID" type="text" />
        
        <property name="chartId" column="CHART_ID" type="text" />
        
        <property name="type" column="REPORT_TYPE" type="text" />
        
    </class>
    
</hibernate-mapping>
